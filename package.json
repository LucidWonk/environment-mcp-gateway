{
  "name": "lucidwonks-environment-mcp-gateway",
  "version": "1.0.0",
  "description": "MCP server providing development environment access for Lucidwonks trading platform",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "node scripts/clean.cjs",
    "clean:all": "node scripts/clean.cjs && npm run clean:modules",
    "clean:modules": "npx rimraf node_modules",
    "clean:cache": "node scripts/clean.cjs && npm cache clean --force"
  },
  "keywords": [
    "mcp",
    "development",
    "environment",
    "git",
    "docker",
    "database",
    "infrastructure"
  ],
  "author": "Lucidwonks",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "pg": "^8.11.3",
    "ssh2": "^1.15.0",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/ssh2": "^1.15.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-editor-support": "^32.0.0-beta.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
